{
  "name": "blablaland-desktop",
  "description": "Version bureau du jeu Blablaland (inclus Flash Player)",
  "author": "Blablaland.fun",
  "version": "1.0.0",
  "license": "MIT",
  "main": "public/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yovach/blablaland-desktop"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "pack": "electron-builder -p never",
    "release": "electron-builder",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev-win": "concurrently \"yarn start\" \"wait-on http://localhost:3000 ; electron .\""
  },
  "devDependencies": {
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^9.8.6",
    "concurrently": "^6.1.0",
    "electron": "^12.0.7",
    "electron-builder": "^22.11.1",
    "gulp": "^4.0.2",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "wait-on": "^5.3.0"
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}